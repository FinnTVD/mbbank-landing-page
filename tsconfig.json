{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./src/*"],
      "grapesjs": ["/src/modules/grapesjs"],
      "grapesjs-preset-webpage": ["/src/modules/grapesjs-preset-webpage"],
      "grapesjs-blocks-basic": ["/src/modules/grapesjs-blocks-basic"],
      "grapesjs-plugin-forms": ["/src/modules/grapesjs-plugin-forms"],
      "grapesjs-plugin-export": ["/src/modules/grapesjs-plugin-export"],
      "grapesjs-component-countdown": [
        "/src/modules/grapesjs-component-countdown"
      ],
      "grapesjs-navbar": ["/src/modules/grapesjs-navbar"],
      "grapesjs-style-bg": ["/src/modules/grapesjs-style-bg"],
      "grapesjs-tabs": ["/src/modules/grapesjs-tabs"],
      "grapesjs-tooltip": ["/src/modules/grapesjs-tooltip"],
      "grapesjs-custom-code": ["/src/modules/grapesjs-custom-code"],
      "grapesjs-touch": ["/src/modules/grapesjs-touch"],
      "grapesjs-parser-postcss": ["/src/modules/grapesjs-parser-postcss"],
      "grapesjs-plugin-ckeditor": ["/src/modules/grapesjs-plugin-ckeditor"]
      // "grapesjs-templates": ["/src/modules/grapesjs-templates"],
      // "grapesjs-project-manager": ["/src/modules/grapesjs-project-manager"],
      // "grapesjs-template-manager": ["/src/modules/grapesjs-template-manager"],
      // "grapesjs-swiper-slider": ["/src/modules/grapesjs-swiper-slider"],
      // "grapesjs-blocks-bootstrap4": ["/src/modules/grapesjs-blocks-bootstrap4"],
      // "grapesjs-lory-slider": ["/src/modules/grapesjs-lory-slider"],
      // "grapesjs-plugin-filestack": ["/src/modules/grapesjs-plugin-filestack"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts"],
  "exclude": ["node_modules"]
}
